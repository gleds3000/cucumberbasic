pipeline{
    agent any
    stages{
        stage('Contrução'){
          steps{
            withMaven(maven:'maven_3_5_2'){
                sh 'mvn clean compile'
            }
          }
        }
        stage('Teste'){
          steps{
            withMaven(maven:'maven_3_5_2'){
              sh 'mvn test'
            }
          }
        }
        stage('Deploy'){
          steps{
            withMaven(maven:'maven_3_5_2'){
                sh 'mvn deploy'
            }
          }
        }
     }
  }    
